{
  "name": "starter-kit",
  "version": "1.0.0",
  "description": "Starter kit for side projects, hackathons, etc.",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "npx parcel build src/index.html --out-dir ./dist",
    "start": "npx parcel src/index.html --out-dir ./dist",
    "docker:build": "docker build -t starter-kit .",
    "docker:run": "docker run --name starter-kit -p 8080:80 --rm starter-kit",
    "hyper:setup": "./scripts/hyper/setup.sh",
    "deploy": "./scripts/hyper/deploy.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "author": "Domenic Matesic <domenic.matesic@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "hyperapp": "^1.2.9",
    "lodash": "^4.17.11",
    "tabletop": "^1.5.2"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "babel-core": "^6.26.3",
    "husky": "^1.2.0",
    "parcel": "^1.10.3",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "tailwindcss": "^0.7.2"
  }
}
